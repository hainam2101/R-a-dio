<Window x:Class="Radio.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Radio"
        mc:Ignorable="d"
        Title="Radio" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="textBlockSong" HorizontalAlignment="Left" Margin="10,46,0,0" TextWrapping="Wrap" Text="Song:" VerticalAlignment="Top" Height="22" Width="38"/>
        <TextBlock x:Name="textBlockSongValue" HorizontalAlignment="Left" Margin="71,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Width="436"><Run Text=""/></TextBlock>
        <TextBlock x:Name="textBlockDJ" HorizontalAlignment="Left" Margin="10,73,0,0" TextWrapping="Wrap" Text="DJ:" VerticalAlignment="Top" Height="22" Width="38"/>
        <TextBlock x:Name="textBlockDJValue" HorizontalAlignment="Left" Margin="71,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Width="436"><Run Text=""/></TextBlock>
        <TextBlock x:Name="textBlockListeners" HorizontalAlignment="Left" Margin="10,100,0,0" TextWrapping="Wrap" Text="Listeners:" VerticalAlignment="Top" Height="22" Width="56"/>
        <TextBlock x:Name="textBlockListenersValue" HorizontalAlignment="Left" Margin="71,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Width="436"><Run Text=""/></TextBlock>
        <TextBlock x:Name="textBlockCurrentTime" HorizontalAlignment="Left" Margin="10,127,0,0" TextWrapping="Wrap" Text="Current second:" VerticalAlignment="Top" Height="22" Width="91"/>
        <TextBlock x:Name="textBlockCurrentTimeValue" HorizontalAlignment="Left" Margin="106,127,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Width="401"><Run Text=""/></TextBlock>
        <TextBlock x:Name="textBlockEndTimeSeconds" HorizontalAlignment="Left" Margin="10,154,0,0" TextWrapping="Wrap" Text="Duration Seconds:" VerticalAlignment="Top" Height="22" Width="111"/>
        <TextBlock x:Name="textBlockEndTimeSecondsValue" HorizontalAlignment="Left" Margin="121,154,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Width="381"><Run Text=""/></TextBlock>


    </Grid>
</Window>
